	"private": true,
	"scripts": {
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"lint": "biome check ./",
		"lint:apply": "biome check ./ --apply-unsafe",
		"format": "biome format ./",
		"format:apply": "biome format ./ --write",
		"start": "react-native start",
		"test": "jest",
		"android:release": "yarn android:clean && yarn android:bundle && yarn android:assemble",
		"android:clean": "cd android && ./gradlew clean && ./gradlew --stop && cd ..",
		"android:bundle": "cd android && ./gradlew bundleRelease && ./gradlew --stop && cd ..",
		"android:assemble": "cd android && ./gradlew assembleRelease && ./gradlew --stop && cd ..",
		"check:tsc": "tsc --noEmit",
		"postinstall": "husky install && patch-package"
	},
